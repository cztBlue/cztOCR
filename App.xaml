<Application
    x:Class="cztOCR.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:cztOCR"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ObjectDataProvider
            x:Key="ConfigDataProvider"
            MethodName="get_Instance"
            ObjectType="{x:Type local:ConfigData}" />
        <ObjectDataProvider
            x:Key="CurrentImageStreamProvider"
            MethodName="get_Instance"
            ObjectType="{x:Type local:CurrentImageStream}" />
    </Application.Resources>
</Application>
